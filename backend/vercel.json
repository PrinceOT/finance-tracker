{
    "version": 2,
    "builds": [
      {
        "src": "backend.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend.js"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination" : "/api"
      }
    ]
  }
  